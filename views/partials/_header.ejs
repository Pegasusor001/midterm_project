<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">Maps App</a>

    <div>
      <ul>
        <a href="/">My Maps</a>
        <a class='createNew' href="/widgets/api/new">Create New Maps</a>
        <a class="search" href="/widgets/api/search">Search</a>
        <a class='myProfile' href="/users/api/profile">My Profile</a>
      </ul>
    </div>

    <div>
        <% if (userId) { %>
          <p>Logged In As: <%=email%></p>
          <form method="POST" action="api/users/logout">
            <button type="submit">Logout</button>
          </form>
        <% } else { %>
          <a href="/api/users/login">Login</a>
          <a href="/api/users/register">Register</a>
        <% } %>
    </div>

  </nav>
</header>
